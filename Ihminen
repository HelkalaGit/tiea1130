package Demo6;


/**
 * @author Joel
 * @version Oct 30, 2018
 * Ihmisen perintä luokka. Enemmänkin esimerkin vuoksi.
 */
public interface Ihminen {
    
    /**
     * Laitetaan ihminen ääntelemään 
     */
    void aantele();
    
    /**
     * Ihminen näyttää hauista
     */
    void naytaHauis();
    
    /**
     * Asetetaan ihmiselle nimi
     * @param nimi ihmisen nimi
     */
    void nimi(String nimi);

    /**
     * Asetetaan ihmisen syntymävuosi
     * @param sAika syntymäaika
     */
    void ika(int sAika);
}
